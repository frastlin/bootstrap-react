{
  "homepage": "https://whatsock.github.io/bootstrap-react",
  "name": "bootstrap-react",
  "version": "0.1.0",
  "private": false,
  "main": "lib/packageIndex.js",
  "module": "lib/packageIndex.js",
  "babel": {
    "presets": [
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "only": [
      "src/AccDC",
      "src/react-wrapped-components",
      "src/packageIndex.js"
    ]
  },
  "dependencies": {
    "bean": "^1.0.15",
    "cheerio": "^1.0.0-rc.2",
    "current-device": "^0.7.9",
    "es6-promise": "^4.2.4",
    "fetch-ie8": "^1.5.0",
    "jquery": "^3.4.1",
    "node-fetch": "^2.6.0",
    "react-html-parser": "^2.0.2",
    "react-native-cheerio": "^1.0.0-rc.4",
    "zenscroll": "^4.0.2"
  },
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-package": "npm run set-production && shx rm -rf lib && shx mkdir lib && npx babel src --out-dir lib --source-maps",
    "set-production": "cross-env-shell node_env=production",
    "format-prettier": "prettier --write ./src/**/*.{js,css,json}",
    "format-html": "js-beautify -r --type='html' ./public/index.html",
    "format": "npm run format-prettier && npm run format-html",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "cross-env": "^6.0.3",
    "debug-js": "^1.0.2",
    "gh-pages": "^2.1.1",
    "prettier": "^1.12.1",
    "prettify": "^0.1.7",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "shx": "^0.3.2"
  }
}
